package com.pk.basics;
class Employee{
	int id;
	String name;
	int age;
	double salary;
	public Employee(int id, String name, int age, double salary) {
		super();
		this.id = id;
		this.name = name;
		this.age = age;
		this.salary = salary;
	}
	
}
